<?xml version="1.0" encoding="utf-8"?>
<web-app
        version="2.5"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>uploadPCDRequest</servlet-name>
        <servlet-class>project.UploadPCDRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadPCDRequest</servlet-name>
        <url-pattern>/uploadPCDRequest</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PCDWorker</servlet-name>
        <servlet-class>project.PCDWorker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PCDWorker</servlet-name>
        <url-pattern>/PCDWorker</url-pattern>
    </servlet-mapping>    
        <servlet>
        <servlet-name>GetPCD</servlet-name>
        <servlet-class>project.GetPCD</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetPCD</servlet-name>
        <url-pattern>/GetPCD/*</url-pattern>
    </servlet-mapping> 
      
    <servlet>
        <servlet-name>GetPointClouds</servlet-name>
        <servlet-class>project.GetPointClouds</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetPointClouds</servlet-name>
        <url-pattern>/GetPointClouds</url-pattern>
    </servlet-mapping> 
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    
</web-app>
